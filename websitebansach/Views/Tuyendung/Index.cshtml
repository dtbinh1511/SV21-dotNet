
@{
    ViewBag.Title = "Tuyển dụng";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section header{
    <style>
        h4 {
            font-size: 2rem;
            border-bottom: 1px solid #f2f2f2;
            line-height: 2;
        }

        p {
            font-size: 1.6rem;
            line-height: 2;
        }
    </style>
}

@section header_top{
    @Html.Action("MainMenu", "Module")
}
@Html.Partial("Breadcrumb")

<div id="app-content">
